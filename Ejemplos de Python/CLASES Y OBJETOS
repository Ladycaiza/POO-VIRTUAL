class Celular:
    def_init_(self, ,arca, modelo, camara);
        self.marca = marca
        self.modelo = modelo
        self.camara = camara

    def funcionxd(xd);
        print(xd)

celular1 = Celular("Samsung","S23", "48MP")
celular2 = Celular("Apple","Iphone 15", "96MP")

print(celular2.marca)



funcionxd("hola jajaja")

